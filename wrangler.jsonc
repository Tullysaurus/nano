{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "nano-proxy",
  "compatibility_date": "2026-02-26",
  "build": {
    "command": "pnpm run build-static"
  },
  "assets": {
    "directory": "./dist",
    "not_found_handling": "single-page-application"
  },
  "routes": [
    {
      "pattern": "proxy.tully.sh",
      "custom_domain": true
    }
  ]
}